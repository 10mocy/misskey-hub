{
	summary: '',
	description: '',
	tags: [
		'pages',
	],
	requireCredential: true,
	req: {
		type: 'object',
		properties: {
			title: {
				type: 'string',
			},
			name: {
				type: 'string',
				minLength: 1,
			},
			summary: {
				type: 'string',
				nullable: true,
			},
			content: {
				type: 'array',
				items: {
					type: 'object',
					additionalProperties: true,
				},
			},
			variables: {
				type: 'array',
				items: {
					type: 'object',
					additionalProperties: true,
				},
			},
			script: {
				type: 'string',
			},
			eyeCatchingImageId: {
				type: 'string',
				format: 'misskey:id',
				nullable: true,
			},
			font: {
				type: 'string',
				enum: [
					'serif',
					'sans-serif',
				],
				default: 'sans-serif',
			},
			alignCenter: {
				type: 'boolean',
				default: false,
			},
			hideTitleWhenPinned: {
				type: 'boolean',
				default: false,
			},
		},
		required: [
			'title',
			'name',
			'content',
			'variables',
			'script',
		],
	},
	res: {
		type: 'object',
		optional: false,
		nullable: false,
		ref: 'Page',
		$ref: '#/components/schemas/Page',
	},
}